{
  "name": "genie-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --configuration=production",
    "vercel-build": "node scripts/set-env.js && ng build --configuration=production",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "serve:ssr:Genie-frontend": "node dist/genie-frontend/server/server.mjs",
    "prepare": "husky",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "env:setup": "node -e \"const fs = require('fs'); const content = fs.readFileSync('src/environments/environment.example.ts', 'utf8'); fs.writeFileSync('src/environments/environment.ts', content); fs.writeFileSync('src/environments/environment.development.ts', content); console.log('Environment files created successfully!');\""
  },
  "lint-staged": {
    "*.{ts,html}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{js,css,scss,json,md}": [
      "prettier --write"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular-eslint/schematics": "^19.6.0",
    "@angular/animations": "^19.2.0",
    "@angular/common": "^19.2.0",
    "@angular/compiler": "^19.2.0",
    "@angular/core": "^19.2.0",
    "@angular/forms": "^19.2.0",
    "@angular/platform-browser": "^19.2.0",
    "@angular/platform-browser-dynamic": "^19.2.0",
    "@angular/platform-server": "^19.2.0",
    "@angular/router": "^19.2.0",
    "@angular/ssr": "^19.2.7",
    "@ngrx/effects": "^19.2.1",
    "@ngrx/operators": "^19.2.1",
    "@ngrx/router-store": "^19.2.1",
    "@ngrx/store": "^19.2.1",
    "@ngrx/store-devtools": "^19.2.1",
    "@primeng/themes": "^19.1.3",
    "@tailwindcss/postcss": "^4.1.8",
    "chart.js": "^4.5.0",
    "express": "^4.18.2",
    "i18n-iso-countries": "^7.14.0",
    "laravel-echo": "^2.1.6",
    "libphonenumber-js": "^1.12.8",
    "postcss": "^8.5.4",
    "primeicons": "^7.0.0",
    "primeng": "^19.1.3",
    "pusher-js": "^8.4.0",
    "rxjs": "~7.8.0",
    "tailwindcss": "^4.1.8",
    "tailwindcss-primeui": "^0.6.1",
    "tslib": "^2.3.0",
    "xlsx": "^0.18.5",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.7",
    "@angular-eslint/builder": "19.6.0",
    "@angular-eslint/eslint-plugin": "19.6.0",
    "@angular-eslint/eslint-plugin-template": "19.6.0",
    "@angular-eslint/template-parser": "19.6.0",
    "@angular/cli": "^19.2.7",
    "@angular/compiler-cli": "^19.2.0",
    "@ngrx/eslint-plugin": "^19.2.1",
    "@types/express": "^4.17.17",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^18.18.0",
    "@types/xlsx": "^0.0.35",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@typescript-eslint/parser": "^7.11.0",
    "eslint": "8.57.0",
    "husky": "^9.1.7",
    "jasmine-core": "~5.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "lint-staged": "^16.1.0",
    "prettier": "^3.5.3",
    "typescript": "~5.7.2"
  }
}
