{
  "version": 2,
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/dist/genie-frontend/browser/assets/$1",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/(.*\\.(js|mjs))$",
      "dest": "/dist/genie-frontend/browser/$1",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable",
        "Content-Type": "application/javascript"
      }
    },
    {
      "src": "/(.*\\.(css|png|jpg|jpeg|gif|ico|svg|woff|woff2|ttf|eot))$",
      "dest": "/dist/genie-frontend/browser/$1",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/dist/genie-frontend/server/server.mjs"
    }
  ]
}
